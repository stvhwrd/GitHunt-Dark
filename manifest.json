{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "GitHunt Dark",
  "short_name": "GitHunt Dark",
  "description": "A new tab page that displays the top trending GitHub repositories.",
  "version": "2.9",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "*://*.github.com/*"
  ],
  "icons": {
    "16": "assets/img/icon16.png",
    "48": "assets/img/icon48.png",
    "128": "assets/img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "githunt-dark.html"
  },
  "homepage_url": "https://github.com/stvhwrd/githunt-dark",
  "options_page": "options.html",
  "browser_action": {
    "default_title": "GitHunt Dark",
    "default_icon": "assets/img/icon128.png"
  }
}